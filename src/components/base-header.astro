---
type Props = {
  class?: string;
};

const { class: _class } = Astro.props as Props;
---

<header class:list={["fixed top-0 left-0 right-0 p-2 z-40", _class]}>
  <div class="flex justify-between gap-2 items-center px-2">
    <slot name="start" />
    <slot name="center" />
    <slot name="end" />
  </div>
</header>
